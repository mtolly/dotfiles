[user]
  name = Michael Tolly
  email = miketolly@gmail.com
[push]
  default = simple
[alias]
  au   = add -u
  d    = diff -M
  dc   = diff --cached -M
  dcs  = diff --cached --stat -M
  ci   = commit
  ls   = log --stat
  tree = log --graph --oneline --all
  s    = status
[core]
  excludesfile = /Users/mtolly/.gitignore
  attributesfile = /Users/mtolly/.gitattributes
[diff "midi"]
  command = /Users/mtolly/bin/mididiff
