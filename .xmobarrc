Config
  { font = "xft:NotoSansCJKJP:pixelsize=16"
  , additionalFonts = []
  , borderColor = "black"
  , border = BottomB
  , bgColor = "#222"
  , fgColor = "#eee"
  , position = TopW L 100
  , commands =
    [ Run Date "%A; %Y %b %_d; %H:%M:%S" "date" 10
    , Run StdinReader
    ]
  , sepChar = "%"
  , alignSep = "}{"
  , template = "%StdinReader%}{%date%"
  }
